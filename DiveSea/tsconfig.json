{
    "compilerOptions": {
      "target": "esnext",
      "lib": ["dom", "esnext"],
      "allowJs": true,
      "jsx": "react-jsx", 
      "moduleResolution": "node",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  